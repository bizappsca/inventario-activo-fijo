											<label value="*" style="font-weight:bold;color:red" />
									</hbox>
									<hbox>
											<textbox id="textBoxTipo" hflex="1" class="multilinea"
													 tooltiptext="Ingrese el Nombre del Tipo" multiline="true"/>
