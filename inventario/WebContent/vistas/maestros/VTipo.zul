<div id="divTipo" apply="controlador.maestros.CTipo">
<groupbox width="50%" mold="3d" title="Tipo">
						<grid width="auto">
							<columns visible="false">
								<column label="" width="25%" />
								<column label="" width="45%" />
								<column label="" width="30%" />
							</columns>
							<rows>
								<row>
									<hbox>
											<label value="Tipo de Equipo"
												style="font-weight:bold;color:green" />
