									<hbox>
									<button autodisable="self" id="btnBuscarTipo" class="btn"
											label="Buscar" tooltiptext="Catalogo Tipos" image="/public/imagenes/botones/buscar.png" />
									</hbox>
								</row>
								</rows>
						</grid>
					<div id="botoneraTipo" />
					<div id="divCatalogoTipo" />
</groupbox>
</div>